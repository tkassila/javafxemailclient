<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="621.0" prefWidth="1040.0" spacing="3.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox prefHeight="30.0" prefWidth="838.0" spacing="5.0">
         <children>
            <HBox fx:id="hboxUserName" spacing="5.0">
               <children>
                  <Label accessibleText="Username or email" text="Email/Username" />
                  <TextField fx:id="textFieldUsername" accessibleHelp="Give Username or email" accessibleText="Username or email" prefHeight="25.0" prefWidth="558.0" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
         </children>
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="32.0" prefWidth="1000.0" spacing="5.0">
         <children>
            <HBox fx:id="hboxPassWord" spacing="5.0">
               <children>
                  <Label accessibleText="password" prefHeight="17.0" prefWidth="86.0" text="Password" />
                  <PasswordField fx:id="textFieldPassWord" accessibleHelp="Give password" accessibleText="password" prefHeight="25.0" prefWidth="558.0" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
         </children>
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="32.0" prefWidth="1000.0" spacing="5.0">
         <children>
            <HBox fx:id="hboxSearch" spacing="5.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="86.0" text="Search" />
                  <TextField fx:id="textFieldSearch" prefHeight="25.0" prefWidth="559.0" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <Button fx:id="buttonHelp" mnemonicParsing="false" onAction="#pressedButtonHelp" prefHeight="25.0" prefWidth="169.0" text="Help" />
         </children>
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </HBox>
      <FlowPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="paneExecution" maxWidth="1.7976931348623157E308" prefHeight="32.0" prefWidth="496.0" spacing="5.0">
               <children>
                  <CheckBox fx:id="checkBoxClearResults" mnemonicParsing="false" prefHeight="17.0" prefWidth="125.0" text="Clear read results" />
                  <Button fx:id="buttonConnect" mnemonicParsing="false" onAction="#pressedButtonConnect" prefHeight="25.0" prefWidth="114.0" text="Start read emails" />
                  <ProgressBar fx:id="progressIndicator" prefHeight="26.0" prefWidth="117.0" progress="0.0" />
                  <Button fx:id="buttonCancelExec" mnemonicParsing="false" onAction="#pressedBttonCancelExec" prefHeight="25.0" prefWidth="116.0" text="Cancel execution" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <HBox fx:id="paneGroup3" maxWidth="1.7976931348623157E308" prefHeight="32.0" prefWidth="346.0" spacing="5.0">
               <children>
                  <Button fx:id="buttonSearch" mnemonicParsing="false" onAction="#pressedButtonSearch" prefHeight="25.0" prefWidth="176.0" text="Start search of emails" />
                  <Button fx:id="buttonNext" mnemonicParsing="false" onAction="#pressedButtonNext" prefHeight="25.0" prefWidth="73.0" text="Next" />
                  <Button fx:id="buttonPrevious" mnemonicParsing="false" onAction="#pressedButtonPrevious" prefHeight="25.0" prefWidth="73.0" text="Previous" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <HBox fx:id="paneGroup2" maxWidth="1.7976931348623157E308" prefHeight="32.0" prefWidth="446.0" spacing="5.0">
               <children>
                  <CheckBox fx:id="checkBoxShowPassword" mnemonicParsing="false" prefHeight="17.0" prefWidth="218.0" text="Show password">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <Button fx:id="buttonReadOldResult" mnemonicParsing="false" onAction="#pressedButtonReadOldResult" prefHeight="25.0" prefWidth="261.0" text="Show old results of .txt file" />
                  <CheckBox fx:id="checkBoxSortAfterDomainNames" layoutX="13.0" layoutY="18.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="263.0" text="Sort after domain names" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
         </children>
      </FlowPane>
      <HBox prefHeight="27.0" prefWidth="1000.0">
         <children>
            <Label fx:id="labelMsg" prefHeight="27.0" prefWidth="656.0" />
         </children>
      </HBox>
      <ListView fx:id="listViews" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
